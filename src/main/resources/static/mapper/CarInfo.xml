<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.teamp.board.dao.CarInfoDAO">
	<select id="LoadAll" resultType="CarInfoDTO">
		SELECT *
		FROM Vehicle_Information
	</select>
	
	<select id="LoadByKwd" parameterType="String" resultType="CarInfoDTO">
		SELECT *
		FROM Photo
		WHERE vehicle_no LIKE #{kwd}		
	</select>
</mapper>